- var items = ["Collections", "Men", "Women", "About","Contact"]
header.header#header
  nav.navbar
    button.button.togglemenubutton#togglemenubutton(type="button" aria-label="toggle menu" aria-haspopup="menu" aria-expanded="false")
      img.buttonicon.togglemenuicon#togglemenuicon(src="./assets/images/icon-menu.svg" alt="")
    .menuoverlay.menuoverlay--hide.menuoverlay--translate#menuoverlay
      ul.mainmenu(aria-labelledby="togglemenubutton")
        each item in items
          li.mainmenu__item
            a.mainmenu__link(href="#")= item
  
  img.logo(src="./assets/images/logo.svg" alt="Logo")
  ul.extramenu
    li.extramenu__item
      button.button.cartbutton#cartbutton(type="button" aria-label="cart menu" aria-haspopup="dialog" aria-expanded="false")
        img.buttonicon(src="./assets/images/icon-cart.svg" alt="cart")

      .cart.hide(role="dialog")
        h2.cart__title Cart
        p.emptydescription Your cart is empty
        form.cartform.hide#cartform
          ul.cartproducts
          button.checkout.button--lighthover Checkout

      .cartindicator.hide 0

    li.extramenu__item
      a.extramenu__link(href="#" aria-label="go to my profile page")
        img.avatar(src="./assets/images/image-avatar.png" alt="avatar")